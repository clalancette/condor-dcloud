NAME = dcloud_gahp
all_target($(NAME))

/* We pick out the specific LIB targets we need to avoid accidently
 * linking against any of the Condor libraries, which we don't want.
 */
CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LIB = -ldcloudapi -lpthread

OBJ = dcloudgahp_main.o dcloudgahp_commands.o PipeBuffer.o dcloudgahp_common.o

public_c_plus_nowrap_target($(NAME),sbin,$(OBJ),$(LIB))

